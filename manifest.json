{
    "name": "Calculator with Fully Fledged Memory",
    "description": "Calculator to test local storage and memory",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/calc16.png",
          "32": "/images/calc32.png",
          "48": "/images/calc48.png",
          "128": "/images/calc128.png"
        }
      },
      "icons": {
        "16": "/images/calci16.png",
        "32": "/images/calci32.png",
        "48": "/images/calci48.png",
        "128": "/images/calci128.png"
      }
     
}

